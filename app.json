{
  "expo": {
    "name": "TimesOver",
    "slug": "TimesOver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/finalicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEE10A" 
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "backgroundColor": "#FEE10A",    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/finalicon.png",
        "backgroundColor": "#FEE10A"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
